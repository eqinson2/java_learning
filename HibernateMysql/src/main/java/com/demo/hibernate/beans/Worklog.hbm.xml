<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<!-- Created by the Middlegen Hibernate plugin 2.1 http://boss.bekk.no/boss/middlegen/ 
		http://www.hibernate.org/ -->

	<class name="com.demo.hibernate.beans.Worklog" table="worklog">

		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native" />
		</id>

		<property name="username" type="java.lang.String" column="username"
			not-null="true" length="50" />
		<property name="year" type="java.lang.Integer" column="year"
			length="10" />
		<property name="month" type="java.lang.Integer" column="month"
			length="10" />
		<property name="day" type="java.lang.Integer" column="day"
			length="10" />
		<property name="title" type="java.lang.String" column="title"
			length="100" />
		<property name="description" type="java.lang.String" column="description"
			length="65535" />
		<property name="logtime" type="java.lang.String" column="logtime"
			length="20" />

		<!-- Associations -->


	</class>
</hibernate-mapping>
